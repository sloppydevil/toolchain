[user]
	name = wayne
	title = architect
	email =  
[alias]
	st = status
	ci = commit
	co = checkout
	last = log -1
	br = branch
	unstage = reset head
	zz = log --graph --pretty=format:\"%Cred%h %C(yellow)%d %Creset%s%Creset %Cgreen%ad %C(bold blue)%an %C(cyan)%cr%Creset\" --date=format:"%Y-%m-%d,%H:%M:%S"
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	flg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --branches --tags --remotes 

[core]
	excludesfile = /Users/Forest/.gitignore
